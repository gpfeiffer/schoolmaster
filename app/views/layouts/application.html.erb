<!DOCTYPE html>
<html>
<head>
  <title>SchoolMaster</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
<div id="header">
  <div id="banner">
    <%= @page_title || "SchoolMaster" %>
  </div>
  <div id="navigation">
    <div id="home">
      <%= link_to 'Home', root_path %> 
    </div>
    <div id="login">
      Sign in
    </div>
  </div>
</div>
<div id="columns">
  <div id="side">
    <h3>Graduate Students</h3>
    <ul><%= render Student.all %></ul>
  </div>
  <div id="main">
    <%= yield %>
  </div>
</div>
</body>
</html>
