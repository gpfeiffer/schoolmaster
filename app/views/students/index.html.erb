<h1>Graduate Students</h1>

<table>
<% @students.each do |student| %>
  <tr>
    <td><%= link_to student, student %></td>
    <td><%= student.registration %></td>
    <td><%= student.programme %></td>
    <td><%= student.email %></td>
    <td><%= student.work_phone %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Student', new_student_path %>
