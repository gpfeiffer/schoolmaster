<h1>Graduate Students</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Registration</th>
    <th>Programme</th>
    <th>Email</th>
    <th>Work phone</th>
    <th>Started on</th>
    <th>Expected on</th>
    <th>Completed on</th>
  </tr>

<% @students.each do |student| %>
  <tr>
    <td><%= link_to student, student %></td>
    <td><%= student.registration %></td>
    <td><%= student.programme %></td>
    <td><%= student.email %></td>
    <td><%= student.work_phone %></td>
    <td><%= student.started_on %></td>
    <td><%= student.expected_on %></td>
    <td><%= student.completed_on %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Student', new_student_path %>
