<!--
  Postgraduate student list.
  Automatically generated by SchoolMaster at <%= Time.now %>
-->

<% @students.each do |student| %>
  <tr>
    <td><%= student %></td>
    <td><%= mail_to student.mailto %></td>
    <td><%= student.work_phone %></td>
    <td><%= student.room %></td>
  </tr>
<% end %>
